<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <VBox fx:id="vBoxMembersAdd" layoutX="7.0" layoutY="6.0" prefHeight="395.0" prefWidth="587.0">
         <children>
            <Label prefHeight="52.0" prefWidth="252.0" text="Added Members">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" />
               </VBox.margin>
            </Label>
            <ListView fx:id="addedMemberList" prefHeight="61.0" prefWidth="282.0" />
            <Label fx:id="lblGroupName" prefHeight="54.0" prefWidth="366.0" text="Label">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="membersComboBox" prefHeight="58.0" prefWidth="583.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </ComboBox>
            <GridPane>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="507.0" minWidth="10.0" prefWidth="481.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="305.0" minWidth="10.0" prefWidth="106.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#btnAddClick" prefHeight="35.0" prefWidth="86.0" text="Add">
